    "class Solution:\n",
    "    def solve(self, root, val):\n",
    "        \n",
    "        if root == None:\n",
    "            return False\n",
    "        else:\n",
    "            if root.val == val:\n",
    "                return True\n",
    "            self.solve(root.left, val)\n",
    "            self.solve(root.right, val)\n",
    "        return False"
   ]
